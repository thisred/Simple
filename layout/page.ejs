<div id="main">
    <div class="content">
        <div class="content_bg">
            <div class="article">
                <%- page.content %>
            </div>
        </div>
        <% if (page.show_updated){ %>
            <div class="post_update">
                最后编辑于<%- date(page.updated, "YYYY年MM月DD日") %>
            </div>
        <% } %>
        <% if(page.comments !== false) { %>
            <div id="waline"></div>
        <% } %>
    </div>
</div>